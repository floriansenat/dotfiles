{
  "ssh_connections": [
    {
      "host": "fsenat",
      "projects": [
        {
          "paths": ["/var/www/fsen.at"]
        }
      ]
    }
  ],
  "theme": {
    "mode": "system",
    "light": "Gruvbox Light",
    "dark": "Zedokai (Filter Spectrum)"
  },
  "ui_font_size": 13,
  "buffer_font_size": 14,
  "cursor_blink": false,
  "relative_line_numbers": true,
  "scrollbar": { "show": "never" },
  "active_pane_modifiers": {
    "inactive_opacity": 0.5
  },
  "tab_bar": {
    "show": false,
    "show_nav_history_buttons": false
  },
  "toolbar": {
    "quick_actions": false
  },
  "outline_panel": { "button": false },
  "project_panel": { "button": false },
  "chat_panel": { "button": false },
  "collaboration_panel": { "button": false },
  "notification_panel": { "button": false },
  "gutter": {
    "code_actions": false,
    "runnables": false,
    "folds": false
  },
  "indent_guides": {
    "line_width": 1,
    "active_line_width": 3
  },
  "code_actions_on_format": {
    "source.fixAll.eslint": true
  },
  "vim_mode": true,
  "git": {
    "inline_blame": { "enabled": false }
  },
  "terminal": {
    "button": false,
    "line_height": "standard",
    "env": { "GIT_EDITOR": "zed-preview --wait" }
  },
  "assistant": {
    "default_model": {
      "provider": "openai",
      "model": "gpt-4o"
    },
    "version": "2",
    "button": false
  },
  "features": {
    "inline_completion_provider": "none"
  }
}
